#-------------------#
# FIRST ROW FORMULA #
#-------------------#

= IF( C6= "", "", 1)

#---------------------------#
# SECOND ROW AND ON FORMULA #
#---------------------------#

= IF( D6= "", "", 


( IF( AND( 
         D6 >= TIMEVALUE( "07:00:01" ), 
         D6 < TIMEVALUE( "08:00:01" ) ) 
= TRUE, 

(G5+1), (G5+0) )))
